/********** PAGE SECTIONS ************************/

#skipnav {
	left:-1000em;
	position:absolute;
	width:20em;
}

#header {
	background-color: #000;
	color: #fff;
	font-weight: bold;
	font-size: 14px;
	margin-top: 0px;
	padding:6px 0 6px 18px;
}

#sidebar {
	width: 16%;
	margin: 0px 3px 0px 0px;
	float:left;
}

#footer {
	font-weight: normal;
	color: #666;
	margin-top:5px;
}

#footer a {
	font-weight: bold;
	font-size: .92em;
}

#content,
#footer {
	margin-left:16.4%;
}

/******************* PAGE FORMATS ************************/

.list {
	padding:10px 15px 15px 100px;
	min-height: 8em;
}

.full {
	padding:10px 15px 15px 30px;
}

.error {
	padding:30px 15px 15px 30px;
	height: 10em;
}

